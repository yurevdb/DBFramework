<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <!--<TargetFrameworks>netstandard2.0;net472;</TargetFrameworks>-->
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>DBF</AssemblyName>
    <RootNamespace>DBF</RootNamespace>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Yuré Vanderbruggen</Authors>
    <Description>A database framework like Entity Framework made for educational purposes</Description>
    <PackageProjectUrl>https://github.com/yurevdb/DBFramework</PackageProjectUrl>
    <RepositoryUrl>https://github.com/yurevdb/DBFramework</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>Database Framework</PackageTags>
    <PackageReleaseNotes>Initial Release</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.CSharp" Version="4.5.0" />
    <PackageReference Include="System.Data.SqlClient" Version="4.6.0" />
  </ItemGroup>

</Project>
